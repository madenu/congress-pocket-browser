<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
  <androidx.appcompat.widget.LinearLayoutCompat
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_weight="0.1"
      android:layout_margin="2dp"
      android:textAlignment="center"
      android:text="@string/members" />

    <androidx.recyclerview.widget.RecyclerView xmlns:app="http://schemas.android.com/apk/res-auto"
      xmlns:tools="http://schemas.android.com/tools"
      android:id="@+id/my_feed_members_list"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_margin="12dp"
      android:name="com.cs5520.numad20su_congressmobile.controllers.MyFeedFragment"
      app:layoutManager="LinearLayoutManager"
      tools:context=".controllers.MyFeedFragment"
      tools:listitem="@layout/card_layout" />

    <TextView
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_weight="0.1"
      android:layout_margin="2dp"
      android:textAlignment="center"
      android:text="@string/bills" />

    <androidx.recyclerview.widget.RecyclerView xmlns:app="http://schemas.android.com/apk/res-auto"
      xmlns:tools="http://schemas.android.com/tools"
      android:id="@+id/my_feed_bills_list"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_margin="12dp"
      android:name="com.cs5520.numad20su_congressmobile.controllers.MyFeedFragment"
      app:layoutManager="LinearLayoutManager"
      tools:context=".controllers.MyFeedFragment"
      tools:listitem="@layout/card_layout" />

    <TextView
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_weight="0.1"
      android:layout_margin="2dp"
      android:textAlignment="center"
      android:text="@string/committees" />

    <androidx.recyclerview.widget.RecyclerView xmlns:app="http://schemas.android.com/apk/res-auto"
      xmlns:tools="http://schemas.android.com/tools"
      android:id="@+id/my_feed_committees_list"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_margin="12dp"
      android:layout_weight="0.2"
      android:name="com.cs5520.numad20su_congressmobile.controllers.MyFeedFragment"
      app:layoutManager="LinearLayoutManager"
      tools:context=".controllers.MyFeedFragment"
      tools:listitem="@layout/card_layout" />

  </androidx.appcompat.widget.LinearLayoutCompat>
</ScrollView>